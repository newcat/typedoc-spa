{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "no-bitwise": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "trailing-comma": false,
    "member-access": false
  }
}
